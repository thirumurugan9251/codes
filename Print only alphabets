public class sample {
    public static void main(String[] args) {
        String input = "thiru12sri"; 
        String result = "";

        for (int i = 0; i < input.length(); i++) {
            char ch = input.charAt(i);
            if ((ch >= 'A' && ch <= 'Z') || (ch >= 'a' && ch <= 'z')) {
                result += ch;
            }
        }

        System.out.println("Only alphabets: " + result);
    }
}
