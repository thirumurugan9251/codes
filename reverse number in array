public class sample{
    public static void main(String[]args){
        int a[]={1,2,3,4,5};
        System.out.println("numbers in reverse order");
         int[] array=new int[a.length];
         int c=0;
        for(int i=a.length-1; i>=0; i--){
            array[c]=a[i];
            c++;
        }
          for(int j=0; j<array.length; j++){
        System.out.print(array[j]+"");
        }
        
    }
}

