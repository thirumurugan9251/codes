import java.util.Scanner;
class sample{
public static void main(String [] args){
    Scanner scan=new Scanner(System.in);
    System.out.println("enter the number");
    int n=scan.nextInt();
    System.out.println("even number 1 to n");
    for(int i=1; i<=n; i++)
    {
        if(i%2==0)
        {
        System.out.println(i);
    }
    
    }
}}
