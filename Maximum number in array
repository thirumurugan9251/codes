public class sample{
    public static void main(String[]args){
        int a[]={21,19,17,10};
        int[] array=new int[a.length];
        int max=a[0];
        for(int i=0; i<a.length; i++){
            if(a[i]>max){
                max=a[i];
            }
            }
            System.out.println(" maximum number " + max);
        }
    }
