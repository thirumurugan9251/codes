public class sample {
    public static void main(String[] args) {
        String input = "thiru9251";
        String result = "";
        for (int i = 0; i < input.length(); i++) {
            char ch = input.charAt(i);
            if (ch >= '0' && ch <= '9') {
                result += ch;
            }
        }
        System.out.println("Only digits: " + result);
    }
}
