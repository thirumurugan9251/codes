public class sample {
    public static void main(String[] args) {
        int[][]matrix={{10,25,3},
                       {4,50,6},
                       {17,8,9}};
        int max=0;
        for(int[]row:matrix){
            for(int num:row){
                if(num>max){
                    max=num;
        }
        }
             System.out.println("largest element:"+max);
              }
             }
             }
