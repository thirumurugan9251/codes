public class sample{
    public static void main(String[]args){
        int a[]={21,19,17,10};
        int[] array=new int[a.length];
        int avg=a[2];
        for(int i=2; i<a.length; i++){
            if(a[i]>avg){
                avg=a[i];
            }
            }
            System.out.println(" average  number " + avg);
        }
    }
    
