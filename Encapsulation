class Student{
    private String name;
    public void setName(String newName){
        name=newName;
    }
    public String getName(){
        return name;
    }}
    public class Encapsulation{
        public static void main(String[]args){
            Student s1=new Student();
            s1.setName("Thiru");
            System.out.println("student name:"+s1.getName());
    }
}
