public class sample{
 public static void main(String[]args){
     int sum=0;
     int[] []a={{1,2,3,4},
             {4,5,6,7},
             {7,8,9,3}};
             
             for(int i=0; i<a.length; i++){
             for(int j=0; j<a.length; j++){
                 sum=sum+a[i][2];
             }
             }
                  System.out.println("total of colum:"+sum);
             }
             }
